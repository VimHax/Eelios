[
	map <- | x: Number, fn : | Number | -> Number | -> Number [ eval fn(x) ],
	
	fn <- [
		| x: Number | -> Number [ eval x + 1 ],
		| x: Number | -> Number [ eval x * 2 ]
	],
	
	a <- [1, 2, 3, 4, 5],
	b <- [],
	c <- [],
	
	idx <- 0,
	while idx < 5 do [
		b[idx] <- map(a[idx], fn[0]),
		c[idx] <- map(a[idx], fn[1]),
		idx <- idx + 1
	],
	
	print a,
	print b,
	print c
]